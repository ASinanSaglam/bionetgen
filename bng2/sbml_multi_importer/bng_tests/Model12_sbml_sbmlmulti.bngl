begin model
begin parameters
k	1.0
end parameters

begin molecule types 
A(a,c~o~p)
B(b,c~o~p)
end molecule types 

begin seed species
B(b,c~o)	50.0
A(a,c~o)	100.0
end seed species 

begin reaction rules 
A(a,c~o) + B(b,c~o)  -> A(a!1,c~o).B(b!1,c~o) 	k
A(a,c~p) + B(b,c~p)  -> A(a!1,c~p).B(c~p!1,b) 	k
end reaction rules 

end model
