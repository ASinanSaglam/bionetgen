version("2.0.27");

begin parameters
L0	602
R0     1204
1kp1	1.661129568106312e-04
km1	  1
end parameters

begin molecule types
  1 1L(r,r,r)
  2 R(l)
end molecule types

begin seed species
  1L(r,r,r) L0
  R(l)     R0
end seed species

begin reaction rules
1L(r,r,r) + R(l) <->  1L(r,r,r!1).R(l!1) 1kp1*km1,km1
end reaction rules

begin observables
 Molecules 3R R().1L()
end observables 

generate_network({overwrite=>1});
simulate_ode({t_end=>4,n_steps=>10});